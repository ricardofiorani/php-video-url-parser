    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Fiorani",
            "email": "ricardo.fiorani@gmail.com"
        }
    ],
    "require": {
